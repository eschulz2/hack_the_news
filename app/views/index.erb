<div class="container">
  <table>
    <% @posts.each do |post| %>
      <tr>
        <td></td>
        <td>^</td>
        <td><%=post.title%><span><%=post.url%></span></td>
      </tr>
      <tr>
        <td colspan="2"></td>
        <td><span>Score</span><a><%= post.user.name%></a><a>Comments</a></td>
      </tr>
      <tr height="5px"></tr>
    <% end %> 
  </table>
</div>
