<div class="container">
  <table>
    <% @posts.each do |post| %>
      <tr>
        <td></td>
        <td>^</td>
        <td><a href="<%= post.url %>"><%= post.title%></a><span><%=post.url%></span></td>
      </tr>
      <tr>
        <td colspan="2"></td>
        <td><span>Score</span><a href="user/<%= post.user_id%>"><%= post.user.name%></a><a href="post/<%=post.id%>/comments">Comments</a></td>
      </tr>
      <tr height="5px"></tr>
    <% end %> 
  </table>
</div>
